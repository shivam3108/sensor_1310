        --common=on
        --sat_reassoc=off
        --plain_char=unsigned
        --asm_directory=${TEMP_ROOT}
        --list_directory=${TEMP_ROOT}/obj
        --temp_directory=${TEMP_ROOT}
        --pp_directory=${TEMP_ROOT}
        --obj_directory=${TEMP_ROOT}/obj
        --preinclude=${PROJECT_ROOT}/Application/subg/features.h

