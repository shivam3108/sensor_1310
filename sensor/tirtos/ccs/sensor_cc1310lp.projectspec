        -DUSE_ICALL
        -DSTACK_LIBRARY
        -DICALL_LITE

        -DRCN_APP_ASSERT
        -DHAL_ASSERT_SPIN
        -DICALL_HOOK_ABORT_FUNC=halAssertHandler
        -Dxdc_runtime_Log_DISABLE_ALL
        -Dxdc_runtime_Assert_DISABLE_ALL
		-DxPREAMBLE_COMPATIBILITY

